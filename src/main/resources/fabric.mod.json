{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": ${authors},
    "contact": ${contact},
    "license": "${license_type}",
    "icon": "${icon_path}",
    "environment": "${environment}",
    "entrypoints": ${entrypoints},
    "mixins": [
        "${mod_id}.mixins.json"
    ],
    "depends": {
        "fabricloader": "~${fabricloader_version}",
        "minecraft": "~${minecraft_version}",
        "java": ">=${java_version}",
        "fabric-api": "*"
    },
    "custom": ${custom}
}